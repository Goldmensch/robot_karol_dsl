// yellow, blue, red, green
main {
    turnLeft()
    loop {
        if isWall() {
            fast {
                2 times { turnRight() }
            test isMark(x) {
                yellow -> { deleteMark() setMark(blue) }
                blue -> { deleteMark() setMark(red) }
                red -> { deleteMark() setMark(green) }
                green -> { deleteMark() setMark(yellow) }
                _ -> setMark(yellow)
            }
            }
        }
        step()
    }
}